; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CuceiTacoTycoonv1"
run/main_scene="res://Scenes/node_2d.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Inventory="*res://Scripts/Inventory.gd"
SuppliesUi="*res://Scripts/SuppliesUI.gd"
ButtonDisplay="*res://Scripts/ButtonDisplay.gd"
TextureButtons="*res://Scripts/TextureButtons.gd"
Spawner="*res://Scripts/Spawner.gd"
PathFollow2d="*res://Scripts/PathFollow2D.gd"
Recipe="*res://Scripts/Recipe.gd"
GlobalProgressBar="*res://Scripts/ProgressBar.gd"
GameLoad="*res://Scripts/GameLoad.gd"
LevelManager="*res://Scripts/LevelManager.gd"
GradientDescent="*res://Scripts/GradientDescent.gd"
GraphPlot="*res://Scripts/GraphPlot.gd"
IngredientsManager="*res://Scripts/IngredientsManager.gd"
EcUtilities="*res://addons/easy_charts/utilities/scripts/ec_utilities.gd"
Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"
multiplayer="*res://Scripts/MultiplayerManager.gd"
client="*res://Scripts/Client.gd"
MultiplayerButton="*res://Scripts/MultiplayerButton.gd"
Tutorial="*res://Scripts/Tutorial.gd"
GrillManager="*res://Scripts/GrillManager.gd"

[display]

window/size/viewport_width=576
window/size/viewport_height=1024
window/stretch/mode="canvas_items"
window/handheld/orientation=5

[editor_plugins]

enabled=PackedStringArray("res://addons/SignalVisualizer/plugin.cfg", "res://addons/easy_charts/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg")

[global_group]

TutorialNodes=""
IngredientsContainer=""
GrillLabels=""
SuppliesNodes=""
Clientes=""
SpritesOrders=""

[gui]

theme/custom_font="res://Fonts/PressStart2P.ttf"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
